{
  "name": "ui-workbench",
  "description": "sandbox environment to experiment new stuff, have fun!",
  "version": "rc.5a",
  "main": "dist/bundle.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard '**/*.js' '**/*.jsx'",
    "prebuild": "rimraf docs",
    "build": "build-storybook -o docs -c .storybook",
    "storybook": "start-storybook -p 6006 -c .storybook --ci",
    "format": "prettier-standard 'src/**/*.js'",
    "profile": "webpack --mode production --progress --profile --json > stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.9.1",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/addon-storysource": "^4.1.11",
    "@storybook/addon-viewport": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "file-loader": "^3.0.1",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.3",
    "prettier-standard": "^9.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "standard": "^12.0.1",
    "styled-components": "^4.1.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "storiesOf"
    ],
    "ignore": [
      "public",
      "dist",
      "docs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/DrDyne/ui-workbench.git",
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "babel-plugin-emotion": "^10.0.7",
    "emotion": "^10.0.7",
    "emotion-theming": "^10.0.7",
    "react-rellax": "^1.0.2"
  }
}
