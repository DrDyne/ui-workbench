{
  "name": "ui-workbench",
  "description": "sandbox environment to experiment new stuff, have fun!",
  "version": "2.0.2",
  "main": "dist/bundle.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard '**/*.js' '**/*.jsx'",
    "prebuild": "rimraf docs",
    "build": "build-storybook -o docs -c .storybook",
    "storybook": "start-storybook -p 6006 -c .storybook --ci",
    "format": "prettier-standard 'src/**/*.js'",
    "profile": "webpack --mode production --progress --profile --json > stats.json",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@material-ui/core": "^3.9.1",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-knobs": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-storyshots": "5.0.3",
    "@storybook/addon-storysource": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "emotion": "^10.0.9",
    "emotion-theming": "^10.0.9",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "file-loader": "^3.0.1",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-emotion": "^10.0.9",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.3",
    "prettier-standard": "^9.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-rellax": "^1.0.2",
    "react-test-renderer": "^16.8.4",
    "react-waypoint": "^9.0.1",
    "standard": "^12.0.1",
    "styled-components": "^4.1.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.3.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "beforeEach",
      "describe",
      "expect",
      "jest",
      "render",
      "storiesOf",
      "test"
    ],
    "ignore": [
      "public",
      "dist",
      "docs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/DrDyne/ui-workbench.git",
  "dependencies": {
    "storybook": "^1.0.0"
  }
}
